{
    "compilerOptions": {
        "target": "es2018",
        "module": "commonjs",
        "lib": [
            "es2018",
        ],
        "moduleResolution": "node",
        "sourceMap": true,
        "inlineSources": false,
        "declaration": true,
        "outDir": "./dist",
        "rootDir": ".",
        "alwaysStrict": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "stripInternal": true,
    }
}
