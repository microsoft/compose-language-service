{
    "compilerOptions": {
        "target": "es2022",
        "lib": [
            "es2022",
        ],
        "module": "nodenext",
        "moduleResolution": "nodenext",
        "sourceMap": true,
        "inlineSources": false,
        "declaration": true,
        "outDir": "./dist",
        "rootDir": ".",
        "alwaysStrict": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "stripInternal": true,
    }
}
